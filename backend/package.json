{
  "name": "rinku-backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "cross-env NODE_ENV=test jest",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "db:model": "sequelize model:generate",
    "db:migration": "sequelize migration:generate",
    "db:seed:make": "sequelize seed:generate"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "pino": "^9.2.0",
    "pino-http": "^9.0.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  }
}
